{
  "collectionName": "components_main_projects",
  "info": {
    "displayName": "Project"
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subTitle": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "description": {
      "type": "blocks",
      "required": true
    },
    "pictures": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "codeUrl": {
      "type": "string",
      "required": true,
      "regex": "^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$"
    },
    "projectUrl": {
      "type": "string",
      "regex": "^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    }
  },
  "config": {}
}
