{
  "collectionName": "components_head_seos",
  "info": {
    "displayName": "SEO"
  },
  "options": {},
  "attributes": {
    "pageTitle": {
      "type": "string",
      "maxLength": 60,
      "required": true
    },
    "pageDescription": {
      "type": "string",
      "maxLength": 160,
      "required": true
    },
    "canonicalUrl": {
      "type": "string",
      "regex": "^(https?|ftp):\\/\\/[^\\s/$.?#].[^\\s]*$"
    },
    "noFollow": {
      "type": "boolean",
      "default": false
    },
    "noIndex": {
      "type": "boolean",
      "default": false
    },
    "shareImages": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    }
  },
  "config": {}
}
